/* pages/mine/mine.wxss */
.footer{
  position: fixed;
  left: 0;
  bottom: 0;
  height: 100rpx;
  width: 100%;
}
.wrap{
  margin: 20rpx 3%;
  width: 94%;
  box-sizing: border-box;
  background:#fff;
  border-radius: 10rpx;
}
.wrap-title{
  box-sizing: border-box;
  padding:20rpx 20rpx;
  width:100%;
  border-bottom: 1px solid #e4e4e4; 
}
.wrap-detail{
  box-sizing: border-box;
  width:100%;
  padding:35rpx 20rpx 35rpx 20rpx;
  font-size:25rpx;
  color: #d5d5d5;
}